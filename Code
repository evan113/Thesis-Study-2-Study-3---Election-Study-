  drop_na(GD_W2, EXTERNAL_PE_W4) %>%
  summarise(correlation_coefficient = cor(GD_W2, EXTERNAL_PE_W4, method = "pearson"),
            p_value = cor.test(GD_W2, EXTERNAL_PE_W4)$p.value,
            lower_ci = cor.test(GD_W2, EXTERNAL_PE_W4)$conf.int[1],
