            p_value = cor.test(GD_W4, EXTERNAL_PE_W4)$p.value,
            lower_ci = cor.test(GD_W4, EXTERNAL_PE_W4)$conf.int[1],
            upper_ci = cor.test(GD_W4, EXTERNAL_PE_W4)$conf.int[2])
